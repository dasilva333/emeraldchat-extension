{
   "background": {
      "persistent": true,
      "scripts": [ "background.js" ]
   },
   "content_scripts": [ {
      "js": [ "content.js" ],
      "matches": [ "https://www.emeraldchat.com/app" ],
      "run_at": "document_end"
   } ],
   "default_locale": "en",
   "description": "__MSG_appDesc__",
   "icons": {
      "128": "icon-128.png",
      "195": "image.png",
      "48": "icon-48.png"
   },
   "manifest_version": 2,
   "name": "__MSG_appName__",
   "page_action": {
      "default_icon": "icon.png",
      "default_title": "__MSG_appEnabled__"
   },
   "permissions": [ "webRequest", "webRequestBlocking", "*://*.emeraldchat.com/*", "declarativeContent" ],
   "version": "1.1.8",
   "web_accessible_resources": [ "icon-48.png", "disabled.png", "/inject.js", "/application-mod.js" ],
   "content_security_policy": "script-src 'self' 'unsafe-eval' https://cdnjs.cloudflare.com; object-src 'self'"
}
